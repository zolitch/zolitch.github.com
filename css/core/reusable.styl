/* Font icons loaded from /fonts/fonts.css use this class too*/

.ico
	inline-block()
	text-align left
	text-decoration inherit
	line-height 1
	-webkit-font-smoothing antialiased
	text-decoration none
	padding-top:1px;

.ico:before
	padding 4px 4px 0 0

a.ico, a.ico span
	cursor: pointer

.ico-pdf
	width 30px
	height 40px
	background-repeat no-repeat
	//background-image url('data:image/png;base64,')
	background-image url(/images/ico-pdf-x2.png)

.icon-awaiting:before
	content: "?";
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;


